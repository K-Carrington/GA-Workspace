<%= form_tag sessions_path, class:"form-inline" do %>
  <div class="form-group">
    <%= text_field_tag :email, nil , placeholder: "Email", class: "form-control"%>
  </div>
  <div class="form-group">
    <%= password_field_tag :password, nil,placeholder: "password", class: "form-control" %>
  </div>
  <div class="btn btn-default"><%= submit_tag "Log In" %></div>
  <%= link_to "Don't have an account?", new_user_path %>
<% end %>

